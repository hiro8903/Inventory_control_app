<% provide(:title, 'メーカー登録') %>
<% provide(:button_text, '新規登録') %>

<h1>新規メーカー登録</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with model: @manufacturer, local: true do |f| %>


 <%= render 'shared/error_messages', object: @manufacturer %>  



      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.submit yield(:button_text), class: "btn btn-primary" %>
    <% end %>

  </div>
</div>
